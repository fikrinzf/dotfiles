# === Aliases ===
# VIM
alias vi="vim"
alias vim="nvim"
# Git
alias log="git log --all --oneline --graph"
# Basic Terminal
alias ls="colorls"
alias ll="exa -l"
alias la="exa -a"
# xampp
alias lampp_start="sudo /opt/lampp/xampp start"
alias lampp_stop="sudo /opt/lampp/xampp stop"
alias lampp_status="sudo /opt/lampp/xampp status"
